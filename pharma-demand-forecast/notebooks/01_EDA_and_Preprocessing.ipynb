{ "cells": [{"cell_type": "code", "metadata": {}, "source": ['\n', 'import pandas as pd\n', 'import matplotlib.pyplot as plt\n', 'import seaborn as sns\n', '\n', "df = pd.read_csv('../data/pharma_sales.csv', parse_dates=['date'])\n", '\n', 'print(df.head())\n', 'print(df.describe())\n', "print(df['region'].value_counts())\n", "print(df['product_name'].value_counts())\n", '\n', '# Plot overall sales trend\n', 'plt.figure(figsize=(12,6))\n', "df.groupby('date')['sales_units'].sum().plot()\n", "plt.title('Total Weekly Sales Over Time')\n", "plt.xlabel('Date')\n", "plt.ylabel('Units Sold')\n", 'plt.grid()\n', 'plt.tight_layout()\n', 'plt.show()\n', '\n', '# Stockout frequency\n', 'plt.figure(figsize=(8,4))\n', "sns.countplot(x='stock_out_flag', data=df)\n", "plt.title('Stockout Distribution')\n", 'plt.show()\n']}], "metadata": {}, "nbformat": 4, "nbformat_minor": 2 }