{ "cells": [{"cell_type": "code", "metadata": {}, "source": ['\n', 'import pandas as pd\n', 'from prophet import Prophet\n', 'from sklearn.metrics import mean_absolute_percentage_error, mean_squared_error, r2_score\n', 'import numpy as np\n', '\n', "df = pd.read_csv('../data/pharma_sales.csv', parse_dates=['date'])\n", '\n', '# Filter one product and region\n', "sub_df = df[(df['region'] == 'North') & (df['product_name'] == 'Antibiotic A')]\n", "sub_df = sub_df[['date', 'sales_units']].rename(columns={'date': 'ds', 'sales_units': 'y'})\n", '\n', '# Split\n', 'train = sub_df[:-12]\n', 'test = sub_df[-12:]\n', '\n', '# Train Prophet\n', 'model = Prophet()\n', 'model.fit(train)\n', '\n', '# Forecast\n', "future = model.make_future_dataframe(periods=12, freq='W')\n", 'forecast = model.predict(future)\n', '\n', '# Evaluation\n', "y_true = test['y'].values\n", "y_pred = forecast[-12:]['yhat'].values\n", 'print("MAPE:", mean_absolute_percentage_error(y_true, y_pred))\n', 'print("RMSE:", np.sqrt(mean_squared_error(y_true, y_pred)))\n', 'print("RÂ²:", r2_score(y_true, y_pred))\n', '\n', '# Plot forecast\n', 'model.plot(forecast)\n']}], "metadata": {}, "nbformat": 4, "nbformat_minor": 2 }